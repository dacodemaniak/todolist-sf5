{% extends "base.html.twig" %}

{% block body %}
	<h2>{{ formTitle }}</h2>
	{{ form_start(formCategory) }}
	    {{ form_widget(formCategory) }}
		<button type="submit" name="submit" class="btn btn-default">{{ buttonTitle }}</button>
	{{ form_end(formCategory) }}
	
	{% if message != "" %}
		<div class="alert alert-success">
			{{ message }}
		</div>
	{% endif %}
{% endblock %}